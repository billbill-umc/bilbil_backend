{
    "presets": [
        [
            "@babel/preset-env", {
                "modules": "cjs",
                "useBuiltIns": "entry",
                "corejs": "3.37.1"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-syntax-top-level-await",
        [
            "module-resolver", {
                "alias": {
                    "@": "./src"
                }
            }
        ]
    ],
    "ignore": ["**/*types.d.js"]
}
